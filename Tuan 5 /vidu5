package nguyenvandat.buoi5;
public class vidu5 {
    public static void main(String[] args) {
        //Các câu lệnh xử lý
        int a = 0, b, c;
        ++a;
        b = ++a;
        c = b++;
        System.out.println(a);
        System.out.println(b);
        System.out.println(c);
        }         
}
